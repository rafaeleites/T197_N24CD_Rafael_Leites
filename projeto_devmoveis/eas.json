{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "store",
      "android": {
        "buildType": "apk" // Garantir que o build seja um APK
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle" // Corrigido para gerar um AAB
      }
    }
  },
  "submit": {
    "production": {}
  }
}